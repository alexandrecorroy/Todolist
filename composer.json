{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/",
            "AppBundle\\DataFixtures\\": "src/DataFixtures/",
            "AppBundle\\Controller\\": "src/Controller/",
            "AppBundle\\Security\\": "src/Security/",
            "AppBundle\\Entity\\": "src/Entity/",
            "AppBundle\\Entity\\Interfaces\\": "src/Entity/Interfaces/",
            "AppBundle\\Form\\": "src/Form/",
            "AppBundle\\Form\\Handler\\": "src/Form/Handler/",
            "AppBundle\\Repository\\": "src/Repository/",
            "AppBundle\\Repository\\Interfaces\\": "src/Repository/Interfaces/",
            "AppBundle\\Service\\": "src/Service/",
            "AppBundle\\Service\\Interfaces\\": "src/Service/Interfaces/",
            "AppBundle\\Listener\\": "src/Listener/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">7.1",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/polyfill-apcu": "^1.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "twig/twig": "^1.0||^2.0",
        "phpunit/phpunit": "^7.4",
        "symfony/phpunit-bridge": "*@dev",
        "snc/redis-bundle": "^2.1",
        "predis/predis": "^1.1",
        "symfony/webpack-encore-pack": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
